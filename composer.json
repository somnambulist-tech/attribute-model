{
    "name": "somnambulist/attribute-model",
    "type": "library",
    "description": "Provides a base model that uses an internal array of attributes with type casting",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "pragmarx/ia-str": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "somnambulist/collection": "^5.0",
        "somnambulist/domain": "^5.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Components\\AttributeModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Components\\AttributeModel\\Tests\\": "tests/"
        }
    }
}
